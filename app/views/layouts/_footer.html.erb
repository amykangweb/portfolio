<!-- contact -->
<div id="contact-info">
    <div class="row">
        <div class="large-12 columns">
          <h2 class="text-center">Contact</h2>
            <div class="line"></div>
            <p class="text-center p"><!-- Contact Description --></p>
        </div>
    </div>
    <div class="row">
        <div class="large-12 columns">
            <div class="row">
                <div class="large-3 medium-3 small-12 columns text-center contact-info-list">
                    <%= link_to "https://github.com/amykangweb" do %>
                        <i class="fa fa-github-alt fa-3x" style="color: #00a29d;"></i>
                    <% end %>
                    <h4>Github</h4>
                </div>
                <div class="large-3 medium-3 small-12 columns text-center contact-info-list">
                    <%= link_to "https://www.linkedin.com/in/selene6023" do %>
                        <i class="fa fa-linkedin fa-3x gray" style="color: #b71c5d;"></i>
                    <% end %>
                    <h4>LinkedIn</h4>
                </div>
                <div class="large-3 medium-3 small-12 columns text-center contact-info-list">
                    <%= link_to "https://twitter.com/selene6023" do %>
                        <i class="fa fa-twitter fa-3x" style="color: #e45801;"></i>
                    <% end %>
                    <h4>Twitter</h4>
                </div>
                <div class="large-3 medium-3 small-12 columns text-center contact-info-list">
                    <%= mail_to "selene6023@gmail.com" do %>
                        <i class="fa fa-envelope fa-3x gray" style="color: #b71c5d;"></i>
                    <% end %>
                    <h4>Email</h4>
                </div>
            </div>

            </div>
        </div>
    </div>
</div>
<!-- /contact -->
