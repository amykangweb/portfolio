<!-- Side Menu -->
<a id="menu-toggle" href="#" class="toggle"><i class="fa fa-bars"></i></a>
<div id="sidebar-wrapper">
    <a id="menu-close" href="#" class="btn btn-default btn-lg pull-right toggle"><i class="fa fa-times"></i></a>
    <ul class="sidebar-nav">
        <li><%= link_to "Home", root_path(:anchor => "top") %>
        </li>
        <li><%= link_to "About", root_path(:anchor => "about") %>
        </li>
        <li><%= link_to "Portfolio", root_path(:anchor => "portfolio") %>
        </li>
        <li><%= link_to "Contact", root_path(:anchor => 'contact-info') %>
        </li>
        <% if user_signed_in? %>
          <% if current_user.editor? %>
            <li><%= link_to "New Project", new_project_path %></li>
          <% end %>
          <li><%= link_to "Sign Out", destroy_user_session_path, method: :delete %></li>
        <% end %>
    </ul>
</div>
